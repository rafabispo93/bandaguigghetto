#social-buttons {bottom: 0; position: absolute;}
#social-buttons ul {list-style: none; margin: 0 auto; overflow: hidden; padding:0; width: 330px;}
#social-buttons ul li {float: right; margin: 0 1em 0 0; overflow: hidden;}
#social-buttons ul li a {float: right; height: 48px; overflow: hidden; text-decoration: none; width: 48px; -moz-transition: all ease-in-out 0.3s; -o-transition: all ease-in-out 0.3s; -webkit-transition: all ease-in-out 0.3s; transition: all ease-in-out 0.3s;}
#social-buttons ul li a img {float: left; width: 100%;}
#social-buttons ul li a:hover {-moz-transition: all ease-in-out 0.3s; -o-transition: all ease-in-out 0.3s; -webkit-transition: all ease-in-out 0.3s; transition: all ease-in-out 0.3s;}

@media only screen and (max-width: 479px){
	
	#social-buttons ul li {float: right; margin: 0 0.3em 0 0; overflow: hidden;}
}

