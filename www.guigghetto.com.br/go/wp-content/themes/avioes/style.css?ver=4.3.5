#common-modal .slideshow .arrow {
	position: fixed;
    top: 30%;
    bottom: 0;
    width: 10%;
    text-align: center;
    opacity: 0.3;
    filter: alpha(opacity=30);
    background:none; /* overriding bootstrap styles */
    background-repeat:no-repeat;
    background-position: 50% 60px;
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
	z-index: 99999;
}

#common-modal .slideshow .arrow {
	opacity: .3;
    filter: alpha(opacity=30);
}

#common-modal .slideshow .arrow:hover {
	opacity: 1;
    filter: alpha(opacity=100);
}

#common-modal .slideshow .prev { 
    background-image:url(assets/images/theme_images/carousel_arrow_left.png); 
    left: 0%;
}
#common-modal .slideshow .prev:hover { 
    background-position: 40% 60px;
}
#common-modal .slideshow .next { 
    background-image:url(assets/images/theme_images/carousel_arrow_right.png);
    right: 40%; 
}
#common-modal .slideshow .next:hover { 
    background-position: 60% 60px;
}

